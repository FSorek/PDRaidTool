<UserControl x:Class="PDRaidTool.UI.PlayerListControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PDRaidTool.UI"
             mc:Ignorable="d" 
             d:DesignHeight="244" d:DesignWidth="220">
    <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="224" VerticalAlignment="Top" Width="199" Margin="10,10,0,0">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="32*"/>
                <RowDefinition Height="32*"/>
                <RowDefinition Height="32*"/>
                <RowDefinition Height="32*"/>
                <RowDefinition Height="32*"/>
                <RowDefinition Height="32*"/>
                <RowDefinition Height="32*"/>
            </Grid.RowDefinitions>
            <TextBlock Text="Player List" TextAlignment="Center" FontSize="20" FontFamily="Arial Black"/>
            <ComboBox Grid.Row="1" Margin="5,5,5,5"/>
            <ComboBox Grid.Row="2" Margin="5,5,5,5"/>
            <ComboBox Grid.Row="3" Margin="5,5,5,5"/>
            <ComboBox Grid.Row="4" Margin="5,5,5,5"/>
            <ComboBox Grid.Row="5" Margin="5,5,5,5"/>
        </Grid>
    </Border>
</UserControl>
